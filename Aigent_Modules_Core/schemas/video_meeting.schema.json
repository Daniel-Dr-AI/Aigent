{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Video Meeting Schema",
  "required": ["id", "join_url"],
  "properties": {
    "id": {"type": "string"},
    "meeting_id": {"type": "string"},
    "topic": {"type": "string"},
    "join_url": {"type": "string", "format": "uri"},
    "start_url": {"type": "string", "format": "uri"},
    "password": {"type": "string"},
    "start_time": {"type": "string", "format": "date-time"},
    "duration": {"type": "number"},
    "timezone": {"type": "string"},
    "settings": {
      "type": "object",
      "properties": {
        "host_video": {"type": "boolean"},
        "participant_video": {"type": "boolean"},
        "waiting_room": {"type": "boolean"},
        "encryption_type": {"type": "string"}
      }
    }
  }
}
