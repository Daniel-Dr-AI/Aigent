# PURPOSE: Enable UTM parameter capture and tracking
# ESSENTIAL: false
# DEFAULT: true
# FALLBACK: false (no marketing attribution)
# SOURCE: ENV_INSTRUCTIONS.md → "Marketing Analytics"
ENABLE_UTM_CAPTURE=true

