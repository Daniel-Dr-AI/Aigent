# ───────────────────────────────────────────────────────────────────────────
# Server & Webhook Configuration
# ───────────────────────────────────────────────────────────────────────────

# PURPOSE: Base URL for your n8n instance (no trailing slash)
# ESSENTIAL: true
# DEFAULT: http://localhost:5678
# FALLBACK: n/a - required for all webhooks
# SOURCE: ENV_INSTRUCTIONS.md → "n8n Base URL"
N8N_BASE_URL=http://localhost:5678

# PURPOSE: Webhook base URL (usually same as N8N_BASE_URL, may differ for reverse proxy)
# ESSENTIAL: true
# DEFAULT: copy from N8N_BASE_URL
# FALLBACK: n/a - required for all webhooks
# SOURCE: ENV_INSTRUCTIONS.md → "n8n Base URL"
N8N_WEBHOOK_URL_BASE=http://localhost:5678

# PURPOSE: CORS allowed origins for webhook endpoints
# ESSENTIAL: false
# DEFAULT: *
# FALLBACK: * (allows all origins - only use for local testing)
# SOURCE: ENV_INSTRUCTIONS.md → "CORS Configuration"
ALLOWED_ORIGINS=*

# ───────────────────────────────────────────────────────────────────────────
# Execution & Logging
# ───────────────────────────────────────────────────────────────────────────

# PURPOSE: Maximum workflow execution time in milliseconds
# ESSENTIAL: false
# DEFAULT: 120000 (2 minutes)
# FALLBACK: n8n default (varies by version)
# SOURCE: ENV_INSTRUCTIONS.md → "Performance Settings"
WORKFLOW_TIMEOUT_MS=120000

# PURPOSE: Whether to save execution data (all/errors/none)
# ESSENTIAL: false
# DEFAULT: all
# FALLBACK: errors (saves space but reduces debuggability)
# SOURCE: ENV_INSTRUCTIONS.md → "Performance Settings"
SAVE_EXECUTION_DATA=all

# PURPOSE: n8n execution data retention in hours
# ESSENTIAL: false (but important for Module 05 multi-day sequences)
# DEFAULT: 336 (14 days - required for Module 05)
# FALLBACK: 168 (7 days - will break Module 05 Day-14 touches)
# SOURCE: ENV_INSTRUCTIONS.md → "Execution Retention"
EXECUTIONS_DATA_MAX_AGE=336

# PURPOSE: Logging verbosity (error/warn/info/debug)
# ESSENTIAL: false
# DEFAULT: info
# FALLBACK: warn (less verbose)
# SOURCE: ENV_INSTRUCTIONS.md → "Performance Settings"
LOG_LEVEL=info

# ───────────────────────────────────────────────────────────────────────────
# Global Feature Toggles
# ───────────────────────────────────────────────────────────────────────────

# PURPOSE: Enable PHI masking in logs and audit trails
# ESSENTIAL: true (for HIPAA compliance)
# DEFAULT: true
# FALLBACK: false (only for non-healthcare testing)
# SOURCE: ENV_INSTRUCTIONS.md → "Security & Compliance"
ENABLE_PHI_MASKING=true

# PURPOSE: Enable automatic phone number normalization to E.164 format
# ESSENTIAL: false
# DEFAULT: true
# FALLBACK: false (phone numbers must be pre-formatted)
